   "source": [
    "list = (1, 2, 3, 4, 5, 6, 7, 8, 9)\n",
    "count_odd = 0\n",
    "count_even = 0\n",
    "for num in list:\n",
    "    if num % 2 == 0:\n",
    "        count_even += 1\n",
    "    else:\n",
    "        count_odd += 1\n",
    "print(\"Even numbers in the list: \",count_even)\n",
    "print(\"Odd numbers in the list: \",count_odd)\n",
    "        "
   ]
  }
